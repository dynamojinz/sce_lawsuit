    <record model="ir.ui.view" id="sce_lawsuit.form">
      <field name="name">Lawsuit form</field>
      <field name="model">sce_lawsuit.lawsuit</field>
      <field name="arch" type="xml">
          <form>
              <header>
                  <field name="state" widget="statusbar" readonly="1"/>
              </header>
              <sheet>
                  <group>
                      <group>
                          <field name="name" string="land name"/>
                          <field name="project_name"/>
                      </group>
                      <group>
                          <field name="state"/>
                      </group>
                  </group>
                  <group>
                      <field name="region"/>
                  </group>
                  <group>
                      <field name="cause"/>
                  </group>
                  <separator string="Arbitration State" />
                  <group>
                      <group>
                          <field name="arbi_operator"/>
                      </group>
                      <group>
                          <field name="arbi_claimant"/>
                          <field name="arbi_respondent"/>
                      </group>
                  </group>
                  <group>
                      <field name="arbi_claim"/>
                  </group>
                  <group>
                      <group>
                          <field name="arbi_regist_date"/>
                      </group>
                      <group>
                          <field name="arbi_open_date"/>
                      </group>
                  </group>
                  <group>
                      <field name="arbi_is_settled"/>
                  </group>
                  <group>
                      <field name="arbi_result"/>
                  </group>
              </sheet>
          </form>
      </field>
    </record>
