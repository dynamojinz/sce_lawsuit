<odoo>
  <data>
      <record id="ir_cron_lawsuit_update_notification" model="ir.cron">
          <field name="name">Lawsuit: Update notification</field>
          <field name="model_id" ref="model_sce_lawsuit_lawsuit"/>
          <field name="state">code</field>
          <field name="code">model.process_update_notify()</field>
          <field name="user_id" ref="base.user_root"/>
          <field name="interval_number">1</field>
          <field name="interval_type">hours</field>
          <field name="numbercall">-1</field>
          <field name="doall" eval="False"/>
      </record>
  </data>
</odoo>
